<keymap version="1" name="Default copy" parent="$default">
  <action id="CollapseAll">
    <keyboard-shortcut first-keystroke="ctrl minus" />
  </action>
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
  </action>
  <action id="CollapseExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl minus" />
  </action>
  <action id="CollapseRegionRecursively">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
  </action>
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="EditorResetFontSize">
    <mouse-shortcut keystroke="control button2" />
  </action>
  <action id="ExpandAll">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl equals" />
    <keyboard-shortcut first-keystroke="ctrl decimal" />
  </action>
  <action id="ExpandAllToLevel1">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="1" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad1" />
    <keyboard-shortcut first-keystroke="ctrl numpad1" />
    <keyboard-shortcut first-keystroke="ctrl numpad0" />
  </action>
  <action id="ExpandAllToLevel2">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="2" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad2" />
    <keyboard-shortcut first-keystroke="ctrl numpad2" />
  </action>
  <action id="ExpandAllToLevel3">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="3" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad3" />
    <keyboard-shortcut first-keystroke="ctrl numpad3" />
  </action>
  <action id="ExpandAllToLevel4">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="4" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad4" />
    <keyboard-shortcut first-keystroke="ctrl numpad4" />
  </action>
  <action id="ExpandAllToLevel5">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="5" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad5" />
    <keyboard-shortcut first-keystroke="ctrl numpad5" />
  </action>
  <action id="ExpandExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="ExpandRegionRecursively">
    <keyboard-shortcut first-keystroke="shift ctrl add" />
  </action>
  <action id="Images.Editor.ZoomIn">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="Images.Editor.ZoomOut">
    <keyboard-shortcut first-keystroke="ctrl minus" />
  </action>
  <action id="Rerun">
    <keyboard-shortcut first-keystroke="ctrl f5" />
    <keyboard-shortcut first-keystroke="ctrl decimal" />
  </action>
</keymap>